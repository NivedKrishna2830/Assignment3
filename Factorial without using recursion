// Factorial of a given number without using recursion
#include<stdio.h>
int fact(int n);
void main()
  {
    int n;
    printf("\n Enter a natural number");
    scanf("%d",&n);
    printf("\n The factorial of the given number is %d", fact(n));
  }
int fact(int n)
  {
    int i;
    int fact=1;
    if(n>1)
      {
        for(i=2;i<=n;i++)
          {
            fact=fact*i;
          }
       }
     return(fact);
  }
